    "react-app-rewired": "^2.1.6",
    "react-dev-utils": "^11.0.0",
    "react-test-renderer": "^17.0.2",
    "start-server-and-test": "^1.12.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "wait-on": "^5.2.0"
  },
  "nyc": {
    "report-dir": "coverage/cypress-coverage",
    "temp-dir": "coverage/.nyc_output"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
