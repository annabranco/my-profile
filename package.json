{
  "name": "anna-branco-profile",
  "author": "Anna Branco <anna.branco@icloud.com> (https://github.com/annabranco)",
  "description": "This is Anna Branco's developer profile",
  "version": "1.3.2",
  "repository": "github:annabranco/my-profile/",
  "homepage": "https://www.annab.dev/",
  "keywords": [
    "frontend",
    "developer",
    "react",
    "curriculum",
    "profile"
  ],
  "bugs": {
    "url": "https://github.com/annabranco/my-profile/issues"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "cypress:start:app": "cross-env BROWSER=none react-scripts -r @cypress/instrument-cra start",
    "cypress:start:wait": "start-server-and-test cypress:start:app http://localhost:3000/my-profile",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "start": "react-scripts start",
    "start:dev": "cross-env REACT_APP_DEV_SERVER=true concurrently \"npm run start:dev-server\" \"npm run start:dev-wait-server\"",
    "start:dev-server": "babel-node ./localService/dev-server",
    "start:dev-wait-server": "wait-on http://localhost:3031/health && npm start",
    "test": "jest --verbose --coverage",
    "test:dev": "jest --silent=false --watch",
    "test:e2e": "npm run cypress:start:wait -- \"cypress run\"",
    "test:e2e:dev": "npm run cypress:start:wait -- \"cypress open\"",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "lodash": "^4.17.19",
    "polished": "^4.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.1",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.12.1",
    "@cypress/code-coverage": "^3.9.5",
    "@cypress/instrument-cra": "^1.4.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.41",
